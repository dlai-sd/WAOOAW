    "expectations": [
      {
        "httpRequest": {
          "method": "GET",
          "path": "/health"
        },
        "httpResponse": {
          "statusCode": 200,
          "body": {
            "status": "healthy",
            "service": "plant"
          }
        }
      },
      {
        "httpRequest": {
          "method": "POST",
          "path": "/api/v1/agents"
        },
        "httpResponse": {
          "statusCode": 201,
          "body": {
